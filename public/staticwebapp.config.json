{
    "routes": [
      {
        "route": "/files/*",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "serve": "/index.html"
      }
    ],
    "mimeTypes": {
      ".wasm": "application/wasm",
      ".js": "application/javascript",
      ".json": "application/json",
      ".gz": "application/gzip",
      ".br": "application/octet-stream"
    },
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/files/*"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    }
  }
  